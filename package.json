{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch --env-file=.env src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
